{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "server": "node server/server.js",
    "server:dev": "nodemon server/server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "install:all": "npm install && npm install -D nodemon concurrently",
    "test:validation": "node test-validation.js",
    "test:validation:debug": "node server/test/debug-validation.js",
    "test:security": "node server/test/security.test.js",
    "test:quick": "node quick-security-test.js",
    "test:server": "node test-server-security.js",
    "test:all": "npm run test:validation && npm run test:validation:debug && npm run test:security",
    "test:manual": "echo 'Run: npm run test:server' && echo 'Then follow: MANUAL_SECURITY_TESTING_GUIDE.md'"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
